{
  "name": "fdutils",
  "version": "1.0.3",
  "description": "前端基础工具库",
  "author": "zhangyunling",
  "main": "dist/fdutils.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "webpack --inline --progress --config build/webpack.prod.conf.js && npm run readme",
    "test": "webpack --inline --progress --config build/webpack.test.conf.js && mocha",
    "lint": "eslint src/",
    "readme": "node scripts/create-readme"
  },
  "gitHooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangyunling/fdutils.git"
  },
  "keywords": [
    "utils",
    "fdutils"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.0.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.1.4",
    "node-notifier": "^5.1.2",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "rimraf": "^2.6.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "dist/"
  ]
}
