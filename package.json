{
  "name": "FEUtils",
  "version": "0.0.1",
  "description": "前端基础工具库",
  "author": "zhangyunling",
  "main": "lib/fe-utils.min.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --host 0.0.0.0",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "build:doc": "node build/doc.js",
    "build:lib": "node build/lib.js",
    "test": "npm run test:unit",
    "test:unit": "mocha-webpack --webpack-config build/webpack.test.conf.js test/unit/specs --recursive --require test/unit/setup/mocha.setup.js",
    "test:unit:karma": "cross-env BABEL_ENV=test TARGET=browser karma start test/unit/setup/karma.conf.js --single-run",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangyunling/FE-Utils.git"
  },
  "keywords": [
    "util",
    "tool",
    "fe-util",
    "common Utils"
  ],
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
